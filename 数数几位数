{                                             {
	int x;                                      	int x;
	int n = 0;                                  	int n = 0;
	scanf("%d", &x);                          	  scanf("%d", &x);                       

	n++;                                        	n++;
	x /= 10;                                    	x /= 10;
	if (x > 0) {                                	while (x > 10) {
		n++;                                        		n++;
		x /= 10;                                  		x /= 10;
		if (x > 0) {                              }
			n++;                                    	printf("%d\n", n);
			x /= 10;                                	return 0;
			if...                                   }
		}
	}

	printf("%d\n", n);
	return 0;
}
